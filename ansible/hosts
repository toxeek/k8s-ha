[load_balancer]
lb1 ansible_host=192.168.1.39 new_hostname=loadbalancer1
lb2 ansible_host=192.168.1.40 new_hostname=loadbalancer2


[control_plane]
control1 ansible_host=192.168.1.43 new_hostname=kcontrolplane1
control2 ansible_host=192.168.1.42 new_hostname=kcontrolplane2
control3 ansible_host=192.168.1.44 new_hostname=kcontrolplane3


[workers]
worker1 ansible_host=192.168.1.45 new_hostname=kworker1
worker2 ansible_host=192.168.1.47 new_hostname=kworker2
[all:vars]
ansible_python_interpreter=/usr/bin/python3
